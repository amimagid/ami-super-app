{
  "name": "ami-super-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "setup-db": "node scripts/setup-db.js",
    "db:start": "docker-compose up -d",
    "db:stop": "docker-compose down",
    "db:reset": "docker-compose down -v && docker-compose up -d",
    "start-all": "docker-compose up -d && sleep 10 && npm run dev",
    "start": "./scripts/start-app.sh",
    "deploy": "./scripts/deploy.sh",
    "deploy:prod": "docker-compose -f docker-compose.prod.yml up -d",
    "logs:prod": "docker-compose -f docker-compose.prod.yml logs -f"
  },
  "dependencies": {
    "@types/node": "^20",
    "@types/nodemailer": "^6.4.17",
    "@types/papaparse": "^5.3.14",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/sequelize": "^4.28.19",
    "autoprefixer": "^10.0.1",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "lucide-react": "^0.294.0",
    "mysql2": "^3.6.5",
    "next": "14.0.4",
    "nodemailer": "^7.0.5",
    "papaparse": "^5.4.1",
    "postcss": "^8",
    "react": "^18",
    "react-dom": "^18",
    "recharts": "^2.15.4",
    "sequelize": "^6.35.1",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  },
  "devDependencies": {
    "eslint": "^8",
    "eslint-config-next": "14.0.4"
  }
}
